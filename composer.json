{
    "name": "nexbit/ext-skeleton",
    "description": "Plesk extension skeleton (Paolo Furini's fork)",
    "type": "project",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "PleskExt\\Skeleton\\": "modules/skeleton/library/"
        }
